\f4\b0\fs26 \cf0 # Baseline weights sum to 1.0\
weights = \{\
    'fundamental': 0.5,\
    'technical':   0.2,\
    'sentiment':   0.2,\
    'macro':       0.05,\
    'esg':         0.05\
\}\
\
# Detect major external events and adjust weights dynamically\
if detect_major_event():\
    weights['macro']     += 0.1\
    weights['sentiment'] += 0.1\
    # Normalize\
    total = sum(weights.values())\
    weights = \{k: v/total for k,v in weights.items()\}\
\
# Machine learning optimization\
ml_weights = train_ml_weights(historical_factor_data)\
weights.update(ml_weights)\
\
# Compute score for each horizon\
factors = fetch_factors_for_ticker(ticker, horizon)\
score = sum(factors[k] * weights[k] for k in weights)\
